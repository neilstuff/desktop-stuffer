{
    "name": "desktop-stuffer",
    "version": "1.0.0",
    "description": "Simple Desktop",
    "main": "main.js",
    "scripts": {
        "pack": "electron-builder --dir",
        "dist": "electron-builder --publish never",
        "start": "electron ."
    },
    "build": {
        "win": {
            "target": [
                {
                    "target": "nsis",
                    "arch": [
                        "x64",
                        "ia32"
                    ]
                }
            ]
        }
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/thesourorange/neilstuff/desktop-stuffer.git"
    },
    "keywords": [
        "Electron",
        "utility"
    ],
    "author": "GitHub",
    "license": "CC0-1.0",
    "dependencies": {
        "adm-zip": "^0.5.16",
        "jszip": "^3.10.1",
        "mime": "^3.0.0",
        "quill": "^2.0.2",
        "unzipper": "^0.11.6"
    },
    "devDependencies": {
        "electron": "^40.0.0",
        "electron-builder": "^26.4.0",
        "node": "^25.4.0"
    },
    "bugs": {
        "url": "https://github.com/thesourorange/neilstuff/desktop-stuffer"
    },
    "homepage": "https://github.com/thesourorange/neilstuff/desktop-stuffer#readme"
}
